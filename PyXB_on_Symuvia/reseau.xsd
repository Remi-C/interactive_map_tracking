    </xsd:keyref>
    <xsd:keyref name="AffectationDestination" refer="ODKey">
      <xsd:selector xpath=".//COUPLE_ENTREE_DESTINATION"/>
      <xsd:field xpath="@id_destination"/>
    </xsd:keyref>
    <xsd:keyref name="AffectationAval" refer="TronconKey">
      <xsd:selector xpath=".//SORTIE_CONNEXION"/>
      <xsd:field xpath="@id_troncon_aval"/>
    </xsd:keyref>
    <xsd:keyref name="CoeffDirTronconAval" refer="TronconKey">
      <xsd:selector xpath=".//COEFF_TRONCON_AVAL"/>
      <xsd:field xpath="@id_aval"/>
    </xsd:keyref>
    <xsd:keyref name="CoeffDirTronconAmont" refer="TronconKey">
      <xsd:selector xpath=".//COEFF_TRONCON_AMONT"/>
      <xsd:field xpath="@id_amont"/>
    </xsd:keyref>
    <xsd:keyref name="MatriceODDestination" refer="ODKey">
      <xsd:selector xpath=".//MATRICE_OD/DESTINATION"/>
      <xsd:field xpath="@sortie"/>
    </xsd:keyref>
    <xsd:keyref name="MatriceODInterne" refer="ParkingGroupKey">
      <xsd:selector xpath=".//MATRICE_OD_INTERNE/DESTINATION"/>
      <xsd:field xpath="@sortie"/>
    </xsd:keyref>
    <xsd:keyref name="TronconTrafic" refer="TronconKey">
      <xsd:selector xpath=".//TRAFICS/TRAFIC/TRONCONS/TRONCON"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:keyref name="TronconItineraireTrafic" refer="TronconKey">
      <xsd:selector xpath=".//ITINERAIRE/TRONCONS_/TRONCON_"/>
      <xsd:field xpath="@libelle"/>
    </xsd:keyref>
    <xsd:keyref name="ElementSimulation" refer="SimulationElementKey">
      <xsd:selector xpath="./SIMULATIONS/SIMULATION/ELEMENTS/ELEMENT"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:keyref name="ExtremiteTrafic" refer="ExtremiteKey">
      <xsd:selector xpath="./TRAFICS/TRAFIC/EXTREMITES/EXTREMITE"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:keyref name="ParkingIndividuelTrafic" refer="ParkingKey">
      <xsd:selector xpath="./TRAFICS/TRAFIC/PARKINGS/PARKING"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:keyref name="ParkingZoneTrafic" refer="ParkingKey">
      <xsd:selector
        xpath="./TRAFICS/TRAFIC/ZONES_DE_TERMINAISON/ZONE_DE_TERMINAISON/GROUPE_PARKING/PARKING"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:keyref name="ZoneTrafic" refer="ZoneKey">
      <xsd:selector xpath="./TRAFICS/TRAFIC/ZONES_DE_TERMINAISON/ZONE_DE_TERMINAISON"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:keyref name="ZoneTronconTrafic" refer="TronconKey">
      <xsd:selector
        xpath="./TRAFICS/TRAFIC/ZONES_DE_TERMINAISON/ZONE_DE_TERMINAISON/TRONCONS/TRONCON"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:keyref name="ConnexionInterneTrafic" refer="ConnexionKey">
      <xsd:selector xpath="./TRAFICS/TRAFIC/CONNEXIONS_INTERNES/CONNEXION_INTERNE"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:key name="SimulationSectionKey">
      <xsd:selector xpath=".//SIMULATIONS/SIMULATION"/>
      <xsd:field xpath="@id"/>
    </xsd:key>
    <xsd:key name="TraficSectionKey">
      <xsd:selector xpath=".//TRAFICS/TRAFIC"/>
      <xsd:field xpath="@id"/>
    </xsd:key>
    <xsd:key name="ReseauSectionKey">
      <xsd:selector xpath=".//RESEAUX/RESEAU"/>
      <xsd:field xpath="@id"/>
    </xsd:key>
    <xsd:keyref name="SimulationSectionKeyRef" refer="SimulationSectionKey">
      <xsd:selector xpath=".//SCENARIOS/SCENARIO"/>
      <xsd:field xpath="@simulation_id"/>
    </xsd:keyref>
    <xsd:keyref name="TraficSectionKeyRef" refer="TraficSectionKey">
      <xsd:selector xpath=".//SCENARIOS/SCENARIO"/>
      <xsd:field xpath="@trafic_id"/>
    </xsd:keyref>
    <xsd:keyref name="ReseauSectionKeyRef" refer="ReseauSectionKey">
      <xsd:selector xpath=".//SCENARIOS/SCENARIO"/>
      <xsd:field xpath="@reseau_id"/>
    </xsd:keyref>
    <xsd:key name="ScenarioKey">
      <xsd:selector xpath=".//SCENARIOS/SCENARIO"/>
      <xsd:field xpath="@id"/>
    </xsd:key>
    <xsd:keyref name="ParkingTypeVehiculeInterditKeyRef" refer="TypeVehiculeKey">
      <xsd:selector xpath=".//PARKING/TYPE_VEHICULE_INTERDIT"/>
      <xsd:field xpath="@type"/>
    </xsd:keyref>
    <xsd:key name="ParkingGroupKey">
      <xsd:selector xpath="./TRAFICS/TRAFIC/ZONES_DE_TERMINAISON/ZONE_DE_TERMINAISON/GROUPE_PARKING"/>
      <xsd:field xpath="@id"/>
    </xsd:key>
    <xsd:key name="ParkingTraficKey">
      <xsd:selector
        xpath="./TRAFICS/TRAFIC/ZONES_DE_TERMINAISON/ZONE_DE_TERMINAISON/GROUPE_PARKING/PARKING | ./TRAFICS/TRAFIC/PARKINGS/PARKING"/>
      <xsd:field xpath="@id"/>
    </xsd:key>
    <xsd:keyref name="ZoneRepartitionOriginesKeyRef" refer="ParkingGroupKey">
      <xsd:selector xpath=".//REPARTITION_ORIGINES/ORIGINE"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:keyref name="ParkingGroupSubElementKeyRef" refer="ParkingKey">
      <xsd:selector xpath=".//GROUPE_PARKING/PARKING"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:key name="RouteKey">
      <xsd:selector xpath="./RESEAUX/RESEAU/ROUTES/ROUTE"/>
      <xsd:field xpath="@id"/>
    </xsd:key>
    <xsd:keyref name="RouteKeyRef" refer="RouteKey">
      <xsd:selector xpath=".//DESTINATION/ROUTE"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:keyref name="TronconRoute" refer="TronconKey">
      <xsd:selector xpath=".//ROUTE/TRONCONS_/TRONCON_"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
  </xsd:element>
  <xsd:complexType name="typeITINERAIRE">
    <xsd:sequence minOccurs="1">
      <xsd:element name="TRONCONS_" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Liste des tronçons constituant l'itinéraire</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="TRONCON_">
              <xsd:complexType>
                <xsd:attribute name="libelle" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant du tronçon</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="coeffAffectation" type="typeFloat01" use="required">
      <xsd:annotation>
        <xsd:documentation>Coefficient d'affectation des vahicules à cet itinéraire. La somme des coefficients des itinéraires doit être égale à 1.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="id" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Identifiant de l'itinéraire défini au niveau du noeud RESEAU/ROUTES. Si cet identifiant n'est pas défini, l'utilisateur doit listé les tronçons de l'itinéraire au niveau du noeud enfant TRONCONS_</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeFLUX">
    <xsd:all>
      <xsd:element minOccurs="0" name="DEMANDES">
        <xsd:annotation>
          <xsd:documentation>Noeud d'accès aux variantes temporelles de la demande pour l'extrémité courante</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="DEMANDE" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Description d'une variante temporelle de demande de l'extrémité courante</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="niveau" type="typeFloatPositif">
                  <xsd:annotation>
                    <xsd:documentation>Niveau de la demande en veh/s</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="duree" type="typeDuree" default="always" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Durée de la variante temporelle
</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="REP_DESTINATIONS" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Noeud d'accès aux variantes temporelles de la répartition vers les destinations pour l'extrémité courante</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="REP_DESTINATION" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Description de la variante temporelle de la répartition vers les destinations</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="DESTINATION" maxOccurs="unbounded">
                    <xsd:annotation>
                      <xsd:documentation>Description d'une destination de l'extrémité courante
</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:annotation>
                          <xsd:documentation>Liste des itinéraires affectés</xsd:documentation>
                        </xsd:annotation>
                        <xsd:element name="ROUTE" type="typeITINERAIRE" maxOccurs="unbounded"
                          minOccurs="0"/>
                      </xsd:sequence>
                      <xsd:attribute name="sortie" type="xsd:string" use="required">
                        <xsd:annotation>
                          <xsd:documentation>Identifiant de la sortie destination</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="coeffOD" type="typeFloat01" use="required">
                        <xsd:annotation>
                          <xsd:documentation>Coefficient de la matrice OD pour l'origine (extrélité courante) et la destination considérée</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="affectation_teta_logit" type="typeFloatStrictPositif"
                        use="optional">
                        <xsd:annotation>
                          <xsd:documentation>Paramètre teta du modèle logit de l'affectation spécifique pour ce couple OD</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="nombre_pluscourtchemin" type="typeIntStrictPositif"
                        use="optional">
                        <xsd:annotation>
                          <xsd:documentation>Nombre de plus court chemin à considérer dans le cas d'un comportment de flux de type 'itineraire' pour ce couple OD</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="affectation_commonality_alpha" type="xsd:float">
                        <xsd:annotation>
                          <xsd:documentation>Valeur maximum du commonality factor admissible pour les itinéraires calculés par le module d'affectation pour ce couple OD</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="affectation_commonality_beta" type="typeFloat01">
                        <xsd:annotation>
                          <xsd:documentation>Valeur du paramètre beta pour le calcul du commonality factor pour ce couple OD</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="affectation_commonality_gamma" type="typeFloat01">
                        <xsd:annotation>
                          <xsd:documentation>Valeur du paramètre gamma pour le calcul du commonality factor pour ce couple OD</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="duree" type="typeDuree" default="always" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Durée de la variante temporelle</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="REP_VOIES" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Noeud d'accès aux variantes temporelles de la répartition par voie du flux provenant de l'extrémité (origine) courante</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="REP_VOIE" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Description d'une variante temporelle de la répartition par voie du flux provenant de l'extrémité (origine) courante</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="duree" type="typeDuree" default="always">
                  <xsd:annotation>
                    <xsd:documentation>Durée de la variante temporelle</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="coeffs" default="always">
                  <xsd:annotation>
                    <xsd:documentation>Liste ordonnée des coefficients de répartition sur les voies du tronçon amont de l'extrémité courante</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="typeFLUX_GLOBAL">
    <xsd:sequence>
      <xsd:element name="FLUX" type="typeFLUX">
        <xsd:annotation>
          <xsd:documentation>Description du flux </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="REP_TYPEVEHICULES">
        <xsd:annotation>
          <xsd:documentation>Description des variantes temporelles de la répartition du flux par type de véhicule (cette répartition est utilisée pour calculer le type de véhicule lors de sa création)</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="REP_TYPEVEHICULE" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Description d'une variante temporelle de la répartition du flux par type de véhicule</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="duree" type="typeDuree">
                  <xsd:annotation>
                    <xsd:documentation>Durée de la variante temporelle</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="coeffs" type="typeFloat01List">
                  <xsd:annotation>
                    <xsd:documentation>Liste ordonnée des coefficients de répartition du flux selon le type de véhicule. La somme doit être égale à 1, la liste doit contenir auntant d'élément que de type de véhicule défini, l'ordre des coefficients doit être identique à celui de la liste des types de véhicules.</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="typeFLUX_TYPEVEHS">
    <xsd:sequence> </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
